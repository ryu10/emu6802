* ram128
* populate MPU int. 128B with the first 128 bytes of Altair Baisc binary
*
        ORG $2000  * somewhere in ram0[] before ABasic inits
SRC     equ *
DST     equ *+2 
*
        ORG $7000
start   ldx #DATA128
        stx SRC
        ldx #0
        stx DST
        ldaa #128
next    ldx SRC
        ldab 0,X
        inc SRC+1  * OK only 128 bytes are transferred
        ldx DST
        stab 0,X
        inc DST+1
        deca
        bne next
        swi
        end
*
DATA128 fcb  $0d, $76, $00, $f3, $7e, $18, $f9, $7e
        fcb  $03, $3c, $00, $00, $48, $38, $00, $2c
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
        fcb  $00, $00, $00, $00, $00, $00, $00, $00
